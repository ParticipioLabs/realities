{
  "name": "realities-ui",
  "version": "0.1.0",
  "description": "A frontend for tribal decentralised organisations.",
  "homepage": "",
  "scripts": {
    "start": "react-scripts start",
    "start:test": "better-npm-run start:test",
    "build": "better-npm-run build",
    "lint": "eslint ."
  },
  "betterScripts": {
    "build": {
      "command": "react-scripts build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start:test": {
      "command": "react-scripts start",
      "env": {
        "REACT_APP_GRAPHQL_ENDPOINT": "http://localhost:3101/graphql",
        "REACT_APP_GRAPHQL_SUBSCRIPTION": "ws://localhost:3101/graphql",
        "REACT_APP_KEYCLOAK_CALLBACK_URL": "http://localhost:2551/auth-callback",
        "REACT_APP_KEYCLOAK_REALM": "platotest",
        "REACT_APP_KEYCLOAK_CLIENT": "realitiestest",
        "PORT": "2551"
      }
    }
  },
  "dependencies": {
    "@apollo/client": "3.1.3",
    "babel-eslint": "^10.1.0",
    "better-npm-run": "^0.1.0",
    "bootstrap": "^4.1.3",
    "browserslist": "^4.14.6",
    "downshift": "6.0.6",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "formik": "2.1.5",
    "graphql": "15.3.0",
    "history": "^4.7.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "mime": "^2.0.3",
    "node-fetch": "^2.6.7",
    "node-sass": "4.14.1",
    "oidc-react": "1.4.0",
    "popper.js": "^1.12.9",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-graph-vis": "1.0.5",
    "react-icons": "3.11.0",
    "react-loader": "2.4.7",
    "react-markdown": "4.3.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "^3.0.0",
    "react-tag-input": "^4.7.2",
    "reactstrap": "8.5.1",
    "sass-loader": "^6.0.6",
    "styled-components": "5.2.0",
    "subscriptions-transport-ws": "^0.9.15",
    "webpack": "4.42.0",
    "yup": "^0.26.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
